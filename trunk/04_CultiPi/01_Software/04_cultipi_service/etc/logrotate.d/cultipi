/var/log/cultipi/cultipi.log {
    daily
    rotate 7
    missingok
    create 644 cultipi cultipi
    compress
    sharedscripts
    postrotate
        chown -R cultipi:cultipi /var/log/cultipi
        chmod 755 /var/log/cultipi
        if [ -x /etc/init.d/cultipi ]; then \
            invoke-rc.d cultipi force-reload > /dev/null 2>&1; \
        fi; \
    endscript
}
