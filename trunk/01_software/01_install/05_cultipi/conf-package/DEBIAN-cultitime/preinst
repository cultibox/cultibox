#!/bin/bash

if [ -f /etc/init.d/cultitime ]; then
    /etc/init.d/cultitime stop
    sleep 2

    # Killing remaining process:
    if [ "`ps ax|grep cultitime|grep tclsh`" != "" ]; then
        for process in `ps ax|grep cultitime| grep tclsh| awk -F" " '{print $1}'`; do
            if [ "$process" != "" ]; then            
                kill -9 $process
            fi
        done
    fi
fi

exit 0
